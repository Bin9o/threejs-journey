#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/atob@2.1.2/node_modules/atob/bin/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/atob@2.1.2/node_modules/atob/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/atob@2.1.2/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules:/Users/Sooia/Projects/threejs-journey/node_modules:/Users/Sooia/Projects/node_modules:/Users/Sooia/node_modules:/Users/node_modules:/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/node_modules/atob/bin/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/node_modules/atob/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/atob@2.1.2/node_modules/atob/bin/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/atob@2.1.2/node_modules/atob/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/atob@2.1.2/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules:/Users/Sooia/Projects/threejs-journey/node_modules:/Users/Sooia/Projects/node_modules:/Users/Sooia/node_modules:/Users/node_modules:/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/node_modules/atob/bin/node_modules:/Users/Sooia/Projects/threejs-journey/04-webpack/node_modules/.pnpm/node_modules/atob/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../atob/bin/atob.js" "$@"
else
  exec node  "$basedir/../atob/bin/atob.js" "$@"
fi
