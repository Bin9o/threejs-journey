#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules/.pnpm/terser@5.10.0/node_modules/terser/bin/node_modules:/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules/.pnpm/terser@5.10.0/node_modules/terser/node_modules:/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules/.pnpm/terser@5.10.0/node_modules:/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules/.pnpm/node_modules:/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules:/Users/Sooia/Projects/threejs-journey/node_modules:/Users/Sooia/Projects/node_modules:/Users/Sooia/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules/.pnpm/terser@5.10.0/node_modules/terser/bin/node_modules:/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules/.pnpm/terser@5.10.0/node_modules/terser/node_modules:/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules/.pnpm/terser@5.10.0/node_modules:/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules/.pnpm/node_modules:/Users/Sooia/Projects/threejs-journey/05-transforms-objects/node_modules:/Users/Sooia/Projects/threejs-journey/node_modules:/Users/Sooia/Projects/node_modules:/Users/Sooia/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/terser" "$@"
else
  exec node  "$basedir/../../bin/terser" "$@"
fi
